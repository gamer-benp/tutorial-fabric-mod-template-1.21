{
	"schemaVersion": 1,
	"id": "tutorialfabricmod",
	"version": "${version}",
	"name": "Tutorial Fabric Mod",
	"description": "My mod to test making a fabric mod using the tutorial by Modding by Kaupenjoe",
	"authors": [
		"Ben.P"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/tutorial-fabric-mod/icon.png",
	"environment": "*",
	"entrypoints": {
