			"net.benp.tutorialmodfabric.TutorialFabricMod"
		],
		"fabric-datagen": [
			"net.benp.tutorialmodfabric.TutorialFabricModDataGenerator"
		],
        "client": [
          "net.benp.tutorialmodfabric.TutorialFabricModClient"
        ]
	},
	"mixins": [
		"tutorial-fabric-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
